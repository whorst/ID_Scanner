    public static void time(long x){
    	try{
    	if(x == System.currentTimeMillis()/1000){
    		System.out.println("yes");
    		System.exit(0);
    	}else{
    		time(x);
    	}
    	}catch(StackOverflowError e){
    		
    		time(x);
    	}
    }
   
	public static void checkTime(long x) {

		try {
			if (x == (System.currentTimeMillis())/1000) {
				System.out.println("hi");
			} else {
				checkTime(x);
			}
		} catch (StackOverflowError e) {

			checkTime(x);

		}
	}

}